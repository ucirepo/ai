    "Factorial number : \n",
    "(defun factorial (num)\n",
    "   (cond ((zerop num) 1)\n",
    "      (t ( * num (factorial (- num 1))))\n",
    "   )\n",
    ")\n",
    "(setq n 6)\n",
    "(format t \"~% Factorial ~d is: ~d\" n (factorial n))"
